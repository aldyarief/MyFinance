<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myfinance">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MyFinance"
        android:usesCleartextTraffic="true">
        <activity android:name=".detailactivity.TransDetail"></activity>
        <activity android:name=".detailactivity.KategoriDetail" />
        <activity android:name=".activity.Kategori" />
        <activity android:name=".activity.Dashboard" />
        <activity android:name=".activity.Transaksi" />
        <activity android:name=".activity.Login" />
        <activity android:name=".activity.MainMenu">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>